<p id="notice"><%= notice %></p>

<h1>Belongs Tos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @belongs_tos.each do |belongs_to| %>
      <tr>
        <td><%= belongs_to.user_id %></td>
        <td><%= belongs_to.student_id %></td>
        <td><%= link_to 'Show', belongs_to %></td>
        <td><%= link_to 'Edit', edit_belongs_to_path(belongs_to) %></td>
        <td><%= link_to 'Destroy', belongs_to, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Belongs To', new_belongs_to_path %>
